{
  "name": "rpcapi",
  "version": "2.4.0",
  "description": "Provides a struture for hosting RPC style APIs, supports both http and websocket access out of the box",
  "repository": {
    "type": "git",
    "url": "https://github.com/jyelewis/RPCAPI"
  },
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "tsc -d && cp ../README.md README.md",
    "pretest": "tsc",
    "test": "ava src/*.test.js src/**/*.test.js src/**/*.itest.js",
    "precoverage": "tsc",
    "coverage": "node_modules/.bin/nyc node_modules/.bin/ava src/*.test.js src/**/*.test.js src/**/*.itest.js"
  },
  "keywords": [
    "rpc",
    "api",
    "websocket",
    "communication",
    "server"
  ],
  "devDependencies": {
    "ava": "^2.4.0",
    "node-fetch": "^2.6.0",
    "nyc": "^14.1.1",
    "socket.io-client": "^2.3.0",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/helmet": "^0.0.45",
    "@types/node-fetch": "^2.5.4",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "coveralls": "^3.0.9",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "socket.io": "^2.3.0"
  },
  "author": "Jye Lewis",
